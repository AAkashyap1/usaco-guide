{
  "include": [
    "src/**/*",
    "gatsby-node.ts",
    "gatsby-browser.tsx",
    "gatsby-config.ts",
    "gatsby-ssr.tsx"
  ],
  "exclude": ["src/functions/**/*"],
  "compilerOptions": {
    "target": "es6",
    "lib": ["es2020", "dom"],
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true
  }
}
